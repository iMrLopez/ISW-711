@model ProyectoMarniLopezLopez.Models.dtb_master

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-icon" data-background-color="green">
                <i class="material-icons">pageview</i>
            </div>
            <div class="card-content">
                <div class="card-title">
                    <h4>Detalles de seleccion</h4>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div>
                            <dl class="dl-horizontal">
                                <dt>
                                    @Html.DisplayNameFor(model => model.dtb_status)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.dtb_status)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.dtb_descr)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.dtb_descr)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.dtb_svc_sts.dtb_svc_sts_dsc)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.dtb_svc_sts.dtb_svc_sts_dsc)
                                </dd>
                                <p>
                                    @Html.ActionLink("Editar", "Edit", new { id = Model.dtb_id }) |
                                    @Html.ActionLink("Volver a la lista", "Index")
                                </p>
                            </dl>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>