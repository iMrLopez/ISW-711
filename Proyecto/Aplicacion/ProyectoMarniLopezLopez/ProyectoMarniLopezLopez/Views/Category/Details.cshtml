@model ProyectoMarniLopezLopez.Models.cat_master

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-icon" data-background-color="green">
                <i class="material-icons">pageview</i>
            </div>
            <div class="card-content">
                <div class="card-title">
                    <h4>Detalles de seleccion</h4>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div>
                            <dl class="dl-horizontal">
                                <dt>
                                    @Html.DisplayNameFor(model => model.cat_dsc)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.cat_dsc)
                                </dd>

                            </dl>
                        </div>
                        <p>
                            @Html.ActionLink("Editar", "Edit", new { id = Model.cat_id }) |
                            @Html.ActionLink("Volver a la lista", "Index")
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>